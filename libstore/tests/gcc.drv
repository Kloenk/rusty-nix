rgetPropagated",""),("depsHostHost",""),("depsHostHostPropagated",""),("depsTargetTarget",""),("depsTargetTargetPropagated",""),("doCheck",""),("doInstallCheck",""),("dontBuild","1"),("dontConfigure","1"),("expand-response-params","/nix/store/i0gfw9q9dz3ybv88g5y4ccvcnrnwjlsw-expand-response-params"),("expandResponseParams","/nix/store/i0gfw9q9dz3ybv88g5y4ccvcnrnwjlsw-expand-response-params/bin/expand-response-params"),("gnugrep_bin","/nix/store/sb3jw7cl7x08lmbwypnvlg75mfwh175h-gnugrep-3.4"),("info","/nix/store/7mjpmdkjlrlq12aj21hkh6hyq1957gla-gcc-wrapper-9.3.0-info"),("installPhase","mkdir -p $out/bin $out/nix-support\n\nwrap() {\n  local dst=\"$1\"\n  local wrapper=\"$2\"\n  export prog=\"$3\"\n  substituteAll \"$wrapper\" \"$out/bin/$dst\"\n  chmod +x \"$out/bin/$dst\"\n}\necho $cc > $out/nix-support/orig-cc\n\nccPath=\"/nix/store/afghxjb159hbb8g02929hhggxcpyzqbl-gcc-9.3.0/bin\"\n# Create symlinks to everything in the bintools wrapper.\nfor bbin in $bintools/bin/*; do\n  mkdir -p \"$out/bin\"\n  ln -s \"$bbin\" \"$out/bin/$(basename $bbin)\"\ndone\n\n# We export environment variables pointing to the wrapped nonstandard\n# cmds, lest some lousy configure script use those to guess compiler\n# version.\nexport named_cc=cc\nexport named_cxx=c++\n\nexport default_cxx_stdlib_compile=\"\"\n\nif [ -e $ccPath/gcc ]; then\n  wrap gcc $wrapper $ccPath/gcc\n  ln -s gcc $out/bin/cc\n  export named_cc=gcc\n  export named_cxx=g++\nelif [ -e $ccPath/clang ]; then\n  wrap clang $wrapper $ccPath/clang\n  ln -s clang $out/bin/cc\n  export named_cc=clang\n  export named_cxx=clang++\nfi\n\nif [ -e $ccPath/g++ ]; then\n  wrap g++ $wrapper $ccPath/g++\n  ln -s g++ $out/bin/c++\nelif [ -e $ccPath/clang++ ]; then\n  wrap clang++ $wrapper $ccPath/clang++\n  ln -s clang++ $out/bin/c++\nfi\n\nif [ -e $ccPath/cpp ]; then\n  wrap cpp $wrapper $ccPath/cpp\nfi\n"),("libc_bin","/nix/store/3imnvgp2dkfpsi75ygmgksn16nksf6ls-glibc-2.30-bin"),("libc_dev","/nix/store/q4s680n208jqq7xk8q9fpndva6zck7lq-glibc-2.30-dev"),("libc_lib","/nix/store/bqbg6hb2jsl3kvf6jgmgfdqy06fpjrrn-glibc-2.30"),("man","/nix/store/8yhqaxhsvm117lhqyx4m0iv8661p62k4-gcc-wrapper-9.3.0-man"),("name","gcc-wrapper-9.3.0"),("nativeBuildInputs",""),("out","/nix/store/x45gpnpvbq0l3b933k187qh53y1fg7ll-gcc-wrapper-9.3.0"),("outputs","out man info"),("patches",""),("pname","gcc-wrapper"),("postFixup","# Backwards compatability for packages expecting this file, e.g. with\n# `$NIX_CC/nix-support/dynamic-linker`.\n#\n# TODO(@Ericson2314): Remove this after stable release and force\n# everyone to refer to bintools-wrapper directly.\nif [[ -f \"$bintools/nix-support/dynamic-linker\" ]]; then\n  ln -s \"$bintools/nix-support/dynamic-linker\" \"$out/nix-support\"\nfi\nif [[ -f \"$bintools/nix-support/dynamic-linker-m32\" ]]; then\n  ln -s \"$bintools/nix-support/dynamic-linker-m32\" \"$out/nix-support\"\nfi\n##\n## General libc support\n##\n\n# The \"-B/nix/store/bqbg6hb2jsl3kvf6jgmgfdqy06fpjrrn-glibc-2.30/lib/\" flag is a quick hack to force gcc to link\n# against the crt1.o from our own glibc, rather than the one in\n# /usr/lib.  (This is only an issue when using an `impure'\n# compiler/linker, i.e., one that searches /usr/lib and so on.)\n#\n# Unfortunately, setting -B appears to override the default search\n# path. Thus, the gcc-specific \"../includes-fixed\" directory is\n# now longer searched and glibc's <limits.h> header fails to\n# compile, because it uses \"#include_next <limits.h>\" to find the\n# limits.h file in ../includes-fixed. To remedy the problem,\n# another -idirafter is necessary to add that directory again.\necho \"-B/nix/store/bqbg6hb2jsl3kvf6jgmgfdqy06fpjrrn-glibc-2.30/lib/\" >> $out/nix-support/libc-cflags\necho \"-idirafter /nix/store/q4s680n208jqq7xk8q9fpndva6zck7lq-glibc-2.30-dev/include\" >> $out/nix-support/libc-cflags\nfor dir in \"/nix/store/afghxjb159hbb8g02929hhggxcpyzqbl-gcc-9.3.0\"/lib/gcc/*/*/include-fixed; do\n  echo '-idirafter' ${dir} >> $out/nix-support/libc-cflags\ndone\n\necho \"/nix/store/bqbg6hb2jsl3kvf6jgmgfdqy06fpjrrn-glibc-2.30\" > $out/nix-support/orig-libc\necho \"/nix/store/q4s680n208jqq7xk8q9fpndva6zck7lq-glibc-2.30-dev\" > $out/nix-support/orig-libc-dev\n##\n## Initial CFLAGS\n##\n\n# GCC shows /nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib in `gcc -print-search-dirs', but not\n# /nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib64 (even though it does actually search there...)..\n# This confuses libtool.  So add it to the compiler tool search\n# path explicitly.\nif [ -e \"/nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib64\" -a ! -L \"/nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib64\" ]; then\n  ccLDFlags+=\" -L/nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib64\"\n  ccCFlags+=\" -B/nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib64\"\nfi\nccLDFlags+=\" -L/nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib\"\nccCFlags+=\" -B/nix/store/rmn2n2sycqviyccnhg85zangw1qpidx0-gcc-9.3.0-lib/lib\"\n\necho \"$ccLDFlags\" > $out/nix-support/cc-ldflags\necho \"$ccCFlags\" > $out/nix-support/cc-cflags\n##\n## Man page and info support\n##\n\nln -s /nix/store/zz822kqqn35935arvlcs4rg1h2yqf2iy-gcc-9.3.0-man $man\nln -s /nix/store/al3i16prvfkljk6wnj3m7mz2gl9yxzij-gcc-9.3.0-info $info\n##\n## Hardening support\n##\n\nexport hardening_unsupported_flags=\"\"\nif [ -e \"$out/nix-support/libc-cflags\" ]; then\n  substituteInPlace \"$out/nix-support/libc-cflags\" --replace $'\\n' ' '\nfi\n\nsubstituteAll /nix/store/b3qxpylf33hnka5nbrgy7f4a3236yjbw-add-flags.sh $out/nix-support/add-flags.sh\nsubstituteAll /nix/store/l1ykn6kd0wgphlz2lz1yrqm002g0g1i2-add-hardening.sh $out/nix-support/add-hardening.sh\nsubstituteAll /nix/store/zsa7rqfzmcfxlyby0dj1al8cxjnmahy8-utils.bash $out/nix-support/utils.bash\n\n##\n## Extra custom steps\n##\n"),("preferLocalBuild","1"),("propagatedBuildInputs","/nix/store/gx2r9xhby24ypk0n8ni3dxljm8mrka53-binutils-wrapper-2.31.1"),("propag